
{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "public/assets/$1" },
    { "src": "/track", "dest": "public/track.html" },
    { "src": "/booking", "dest": "public/booking.html" },
    { "src": "/payment", "dest": "public/payment.html" },
    { "src": "/services", "dest": "public/services.html" },
    { "src": "/contact", "dest": "public/contact.html" },
    { "src": "/about", "dest": "public/about.html" },
    { "src": "/faq", "dest": "public/faq.html" },
    { "src": "/careers", "dest": "public/careers.html" },
    { "src": "/Player/(.*)", "dest": "public/index.html" },
    { "src": "/(.*)", "dest": "public/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
